type SogcPublication = record {
    sogc_id: nat64;
    publication_sogc_date: nat64;
    daily_number: nat64;
    publication_date: nat64;
    mutations: text;
    description: text;
};

service : {
    "create_sogc_pubblication" : (SogcPublication) -> (SogcPublication);
    "get_sogc_pubblication" : (nat64) -> (opt SogcPublication) query;
}
