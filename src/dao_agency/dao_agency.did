type Role = variant {
    Member: null;
    Board: null;
};

type User = record {
    id: text;
    role: Role;
};

type DaoAssociationInitArgs = record {
    name : text;
    address : text;
    zip : nat32;
    town : text;
    uid: text;
    ch_id: text;
    frc_id: nat32;
    purpose: text;
    members: vec User;
    documents: vec nat32;
};

service : {
    "create_dao_association" : (DaoAssociationInitArgs) -> (variant { Ok: principal; Err: text });
}
